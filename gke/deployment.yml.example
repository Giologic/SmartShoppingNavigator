apiVersion: apps/v1beta2
kind: Deployment
metadata:
  name: iost-worker
spec:
  replicas: 2
  template:
    metadata:
      labels:
        app: iost-worker
    spec:
      containers:
      - name: iost-worker
        image: gcr.io/MY-PROJECT-ID/iost-worker:latest
        env:
        - name: PROJECT
          value: MY-PROJECT-ID
        - name: INPUT_SUBSCRIPTION
          value: MY-SUBSCRIPTION
        - name: BUCKET
          value: my-bucket
        - name: BLOCKS_URL
          value: "https://xxxxxblocksboardxxxxxx.magellanic-clouts.net/flows/my-flow.json"
        - name: BLOCKS_TOKEN
          value: xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx
        - name: ML_MODEL
          value: model_name
        - name: IOT_REGISTRY
          value: registry_name
        resources:
          requests:
            cpu: "1m"
